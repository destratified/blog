        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--oswald)",
          "fontSize": "var(--wp--custom--typography--font-size--heading-six)",
          "lineHeight": "var(--wp--custom--typography--line-height--heading-six)"
        },
        "color": {
          "text": "var(--wp--preset--color--heading)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--heading)"
        },
        "typography": {
