          "color": {
            "text": "var(--wp--preset--color--secondary)"
          }
        },
        ":focus": {
          "typography": {
            "textDecoration": "none"
          },
          "color": {
            "text": "var(--wp--preset--color--secondary)"
          }
        },
        ":active": {
